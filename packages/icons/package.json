{
  "name": "@pluralsight/icons",
  "version": "0.0.0",
  "description": "Icons for Pluralsight.",
  "main": "build/react/index.js",
  "module": "build/react/wrapper.js",
  "types": "build/react/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/react/wrapper.js",
      "require": "./build/react/index.js",
      "types": "./build/react/index.d.ts"
    },
    "./react": {
      "import": "./build/react/wrapper.js",
      "require": "./build/react/index.js",
      "types": "./build/react/index.d.ts"
    },
    "./react/*": {
      "import": [
        "./build/react/*/wrapper.js",
        "./build/react/*.js"
      ],
      "require": [
        "./build/react/*/index.js",
        "./build/react/*.js"
      ],
      "types": [
        "./build/react/*/index.d.ts",
        "./build/react/*.d.ts"
      ]
    },
    "./react/arrows/*": "./build/react/arrows/*.js",
    "./react/audio-video/*": "./build/react/audio-video/*.js",
    "./react/brands/*": "./build/react/brands/*.js",
    "./react/common-actions/*": "./build/react/common-actions/*.js",
    "./react/communication/*": "./build/react/communication/*.js",
    "./react/editor/*": "./build/react/editor/*.js",
    "./react/navigation/*": "./build/react/navigation/*.js",
    "./react/social/*": "./build/react/social/*.js",
    "./react/ui-actions/*": "./build/react/ui-actions/*.js",
    "./react/*.js": "./build/react/*.js",
    "./svelte": {
      "import": "./build/svelte/wrapper.js",
      "require": "./build/svelte/index.js",
      "types": "./build/svelte/index.d.ts"
    },
    "./svelte/*": "./build/svelte/*.svelte",
    "./string": {
      "import": "./build/string/wrapper.js",
      "require": "./build/string/index.js",
      "types": "./build/string/index.d.ts"
    },
    "./svg/*": "./build/svg/*",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "build/react/index.d.ts": [
        "build/react/index.d.ts"
      ],
      "*": [
        "build/*",
        "build/*/index.d.ts"
      ]
    }
  },
  "files": [
    "build/react/**/*",
    "build/string/**/*",
    "build/svelte/**/*",
    "build/svg/**/*"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "bun run clean && bun run build:svg && bun run build:react && bun run build:svelte && bun run build:strings && bun run build:ts && bun run create:entryFiles",
    "build:ts": "tsc --project ./tsconfig.build.json",
    "build:strings": "bun ./utils/svgToStrings.mjs",
    "build:react": "bun ./utils/svgToReact.mjs",
    "build:svelte": "bun ./utils/svgToSvelte.mjs",
    "build:svg": "svgo --recursive --folder src --output build/svg",
    "clean": "rm -rf build && rm -rf npm",
    "create:entryFiles": "bun ./utils/postbuild.mjs",
    "lint:js": "eslint --max-warnings 0 \"./**/*.@(cjs|mjs|ts)\"",
    "lint:ts": "tsc --project tsconfig.json --noEmit",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "tslib": "latest"
  },
  "peerDependencies": {
    "react": "*"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@svgr/core": "^8.0.0",
    "@svgr/plugin-jsx": "^8.0.1",
    "@types/fs-extra": "^11.0.1",
    "@types/react": "latest",
    "@types/svgo": "^3",
    "eslint": "^8.39.0",
    "fs-extra": "^11.1.0",
    "svelte": "^4.1.0",
    "svgo": "^3.0.2"
  },
  "keywords": [
    "pluralsight",
    "pluralsight design",
    "pluralsight icons",
    "design system",
    "design icons"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Design System Team",
  "bugs": {
    "url": "https://github.com/pluralsight/pando/issues"
  },
  "homepage": "https://design.pluralsight.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/pluralsight/pando.git",
    "directory": "packages/icons"
  },
  "license": "Apache 2.0",
  "packageManager": "bun@1.0.23"
}
