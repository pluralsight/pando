{
  "name": "@pluralsight/panda-preset",
  "version": "0.0.0",
  "description": "Official Panda-CSS preset for the Pando Design System.",
  "author": "Design System Team",
  "bugs": {
    "url": "https://github.com/pluralsight/pando/issues"
  },
  "homepage": "https://design.pluralsight.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/pluralsight/pando.git",
    "directory": "packages/panda-preset"
  },
  "license": "Apache 2.0",
  "main": "index.ts",
  "module": "index.ts",
  "sideEffects": false,
  "browserslist": [
    "> 0.25%",
    "not dead"
  ],
  "exports": {
    ".": {
      "import": "./index.ts",
      "require": "./index.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "main": "npm/node/index.js",
    "module": "npm/browser/index.js",
    "types": "npm/browser/index.d.ts",
    "exports": {
      ".": {
        "import": "./npm/browser/index.js",
        "require": "./npm/node/index.js",
        "types": "./npm/browser/index.d.ts"
      }
    }
  },
  "files": [
    "npm/**/*",
    "src/**/*",
    "index.ts"
  ],
  "scripts": {
    "build": "bun run clean && bun run compile:es && bun run compile:cjs",
    "clean": "rm -rf ./npm",
    "compile:es": "tsc --project tsconfig.build.json",
    "compile:cjs": "tsc --project tsconfig.cjs.json",
    "lint:js": "eslint --max-warnings 0 \"./**/*.ts\"",
    "lint:ts": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "tslib": "latest"
  },
  "peerDependencies": {
    "@pandacss/dev": "*",
    "autoprefixer": "*",
    "postcss": "*"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.16.0",
    "@pandacss/types": "^0.15.3",
    "bun-types": "^1.0.6"
  },
  "keywords": [
    "pluralsight",
    "pluralsight design",
    "pando",
    "panda-css",
    "pando preset",
    "panda-preset"
  ],
  "packageManager": "bun@1.0.6"
}
