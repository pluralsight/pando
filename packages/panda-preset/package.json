{
  "name": "@pluralsight/panda-preset",
  "version": "0.0.0",
  "description": "Official Panda-CSS preset for the Pando Design System.",
  "author": "Design System Team",
  "bugs": {
    "url": "https://github.com/pluralsight/pando/issues"
  },
  "homepage": "https://design.pluralsight.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/pluralsight/pando.git",
    "directory": "packages/panda-preset"
  },
  "license": "Apache 2.0",
  "browserslist": "> 0.25%, not dead",
  "sideEffects": [
    "root.css"
  ],
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "types": "build/legacy/index.d.ts",
    "main": "build/legacy/index.cjs",
    "module": "build/legacy/index.js",
    "exports": {
      ".": {
        "import": {
          "types": "./build/modern/index.d.ts",
          "default": "./build/modern/index.js"
        },
        "require": {
          "types": "./build/modern/index.d.cts",
          "default": "./build/modern/index.cjs"
        }
      },
      "./pando.css": "./root.css",
      "./package.json": "./package.json",
      "./tsconfig.pando.json": "./tsconfig.pando.json"
    },
    "typesVersions": {
      "*": {
        ".": [
          "build/legacy/index.d.ts"
        ]
      }
    }
  },
  "files": [
    "build/**/*",
    "src/**/*",
    "index.ts",
    "root.css",
    "tsconfig.pando.json"
  ],
  "scripts": {
    "build": "tsup --experimental-dts",
    "clean": "rm -rf ./build",
    "lint:js": "eslint --max-warnings 0 \"./**/*.@(ts|tsx)\"",
    "lint:ts": "tsc --project tsconfig.lint.json --noEmit",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "tslib": "latest"
  },
  "peerDependencies": {
    "@pandacss/dev": "*",
    "autoprefixer": "*",
    "postcss": "*"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.38.3",
    "@pandacss/types": "^0.34.1",
    "@pandacss/dev": "^0.33.0",
    "@pando/configs": "workspace:*",
    "bun-types": "^1.0.6",
    "tsup": "^8.0.1"
  },
  "keywords": [
    "pluralsight",
    "pluralsight design",
    "pando",
    "panda-css",
    "pando preset",
    "panda-preset"
  ],
  "packageManager": "pnpm@8.15.1"
}
