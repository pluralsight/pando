    "test": "bun test --coverage"
  },
  "dependencies": {
    "tslib": "latest",
    "yargs": "^17.7.2"
  },
  "devDependencies": {},
  "keywords": [
    "pluralsight",
    "pluralsight design",
    "pando",
    "pando cli"
  ],
  "packageManager": "pnpm@8.6.11"
