{
  "name": "@pluralsight/react",
  "version": "0.0.0",
  "description": "A React component library for Pando.",
  "license": "Apache 2.0",
  "author": "Pando Team",
  "homepage": "https://design.pluralsight.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/pluralsight/pando.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/pluralsight/pando/issues"
  },
  "browserslist": "> 0.25%, not dead",
  "sideEffects": false,
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "types": "build/legacy/index.d.ts",
    "main": "build/legacy/index.cjs",
    "module": "build/legacy/index.js",
    "exports": {
      ".": {
        "import": {
          "types": "./build/modern/index.d.ts",
          "default": "./build/modern/index.js"
        },
        "require": {
          "types": "./build/modern/index.d.cts",
          "default": "./build/modern/index.cjs"
        }
      },
      "./package.json": "./package.json"
    },
    "typesVersions": {
      "*": {
        ".": [
          "npm/types/src/types.d.ts"
        ]
      }
    }
  },
  "files": [
    "build/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsup --experimental-dts",
    "build:deprecated": "bun run clean && bun run compile:types && bun run bundle && bun run clean:allUnused",
    "bundle:deprecated": "rollup --config rollup.config.mjs",
    "clean": "rm -rf ./build",
    "clean:allUnused": "bun run clean:unusedFiles && bun run clean:unusedDirs",
    "clean:unusedFiles": "rm -rf npm/types/index.js",
    "clean:unusedDirs": "bun run clean:unusedDirs:ru && bun run clean:unusedDirs:shared",
    "clean:unusedDirs:ru": "rm -rf npm/types/react-utils",
    "clean:unusedDirs:shared": "rm -rf npm/types/shared",
    "compile:types": "tsc --project tsconfig.build.json",
    "lint:js": "eslint --max-warnings 0 \"./**/*.@(ts|tsx)\"",
    "lint:ts": "tsc --project tsconfig.json --noEmit",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.0.1",
    "@pluralsight/react-aria": "workspace:*",
    "@pluralsight/react-utils": "workspace:*",
    "tslib": "latest"
  },
  "peerDependencies": {
    "@pluralsight/headless-styles": "*",
    "@pluralsight/icons": "*",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.20.13",
    "@microsoft/api-extractor": "^7.38.3",
    "@pluralsight/headless-styles": "workspace:*",
    "@pluralsight/icons": "0.7.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.0",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "core-js": "^3.30.2",
    "esbuild-plugin-file-path-extensions": "^1.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "^4.0.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-node-externals": "6.1.2",
    "tsup": "^8.0.1"
  },
  "keywords": [
    "pluralsight",
    "pluralsight design",
    "pluralsight react utils",
    "react",
    "utils",
    "react-utils",
    "custom hooks",
    "hooks",
    "react hooks",
    "components",
    "ui components"
  ],
  "packageManager": "bun@1.0.6"
}
