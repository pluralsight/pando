{
  "name": "@pluralsight/react",
  "version": "0.0.0",
  "description": "A React component library for Pando.",
  "license": "Apache 2.0",
  "author": "Pando Team",
  "homepage": "https://design.pluralsight.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/pluralsight/pando.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/pluralsight/pando/issues"
  },
  "browserslist": "> 0.25%, not dead",
  "sideEffects": false,
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "types": "build/legacy/index.d.ts",
    "main": "build/legacy/index.cjs",
    "module": "build/legacy/index.js",
    "exports": {
      ".": {
        "import": {
          "types": "./build/modern/index.d.ts",
          "default": "./build/modern/index.js"
        },
        "require": {
          "types": "./build/modern/index.d.cts",
          "default": "./build/modern/index.cjs"
        }
      },
      "./package.json": "./package.json"
    },
    "typesVersions": {
      "*": {
        ".": [
          "npm/types/src/types.d.ts"
        ]
      }
    }
  },
  "files": [
    "build/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsup --experimental-dts",
    "clean": "rm -rf ./build",
    "generate:panda": "panda codegen",
    "lint:js": "eslint --max-warnings 0 \"./**/*.@(ts|tsx)\"",
    "lint:ts": "tsc --project tsconfig.lint.json --noEmit",
    "prepare": "panda codegen",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.0.1",
    "@pluralsight/react-aria": "workspace:*",
    "@pluralsight/react-utils": "workspace:*",
    "tslib": "latest"
  },
  "peerDependencies": {
    "@pluralsight/icons": "*",
    "@pluralsight/panda-preset": "*",
    "@pandacss/dev": "*",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.20.13",
    "@microsoft/api-extractor": "^7.38.3",
    "@pandacss/dev": "^0.24.0",
    "@pando/configs": "workspace:*",
    "@pluralsight/headless-styles": "workspace:*",
    "@pluralsight/panda-preset": "workspace:*",
    "@pluralsight/icons": "0.7.1",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "core-js": "^3.30.2",
    "esbuild-plugin-file-path-extensions": "^2.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "^8.0.1"
  },
  "keywords": [
    "pluralsight",
    "pluralsight design",
    "pluralsight react utils",
    "react",
    "utils",
    "react-utils",
    "custom hooks",
    "hooks",
    "react hooks",
    "components",
    "ui components"
  ],
  "packageManager": "bun@1.0.14"
}
